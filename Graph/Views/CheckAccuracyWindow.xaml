        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graph.Views"
        xmlns:vm="clr-namespace:Graph.VM"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="CheckAccuracyWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <oxy:PlotView Grid.Row="0">
            
        </oxy:PlotView>
        <DataGrid Grid.Row="1" x:Name="TableData" ItemsSource="{Binding Table}">
            
        </DataGrid>
    </Grid>
</Window>
